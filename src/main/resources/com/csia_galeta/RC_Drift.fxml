<?xml version="1.0" encoding="UTF-8"?>
<!-- XML declaration specifying encoding and version -->

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<!-- Import statements for JavaFX classes used in the FXML -->

<AnchorPane prefHeight="307.0" prefWidth="559.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.csia_galeta.controllers.RC_DriftController">
   <!-- AnchorPane element with specific attributes for preferred height, width, and controller class -->

   <children>
      <!-- Child elements of the AnchorPane -->

      <Label layoutX="363.0" layoutY="161.0" text="Application V1">
         <!-- Label element for displaying "Application V1" with specific layout positions and text -->
         <font>
            <Font size="16.0" />
            <!-- Font properties for the label text -->
         </font>
      </Label>

      <AnchorPane layoutX="32.0" layoutY="63.0" prefHeight="181.0" prefWidth="255.0" style="-fx-background-color: gray; -fx-background-radius: 5;">
         <!-- Nested AnchorPane element with specific layout positions, preferred height, width, and style -->
         <children>
            <!-- Child elements of the nested AnchorPane -->

            <Button fx:id="addCompetitionBtn" layoutX="43.0" layoutY="39.0" mnemonicParsing="false" onAction="#openAddCompetitionWindow" style="-fx-background-color: black;" text="New DRIFT event " textFill="WHITE">
               <!-- Button element for adding a new DRIFT event with specific layout positions, action event handler, text, style, and text fill -->
               <font>
                  <Font size="20.0" />
                  <!-- Font properties for the button text -->
               </font>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                  <!-- Padding properties for the button -->
               </padding>
            </Button>

            <Button fx:id="prevCompetitionBtn" layoutX="22.0" layoutY="106.0" mnemonicParsing="false" onAction="#openPrevCompetitions" prefHeight="36.0" prefWidth="212.0" style="-fx-background-color: black;" text="Prev. competitions" textFill="WHITE">
               <!-- Button element for opening previous competitions with specific layout positions, action event handler, preferred height, width, style, and text fill -->
               <font>
                  <Font size="20.0" />
                  <!-- Font properties for the button text -->
               </font>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                  <!-- Padding properties for the button -->
               </padding>
            </Button>

         </children>
      </AnchorPane>

      <Label layoutX="305.0" layoutY="127.0" text="DRIFT EVENT CONTROL">
         <!-- Label element for displaying "DRIFT EVENT CONTROL" with specific layout positions and text -->
         <font>
            <Font name="System Bold" size="20.0" />
            <!-- Font properties for the label text -->
         </font>
      </Label>

   </children>
</AnchorPane>
