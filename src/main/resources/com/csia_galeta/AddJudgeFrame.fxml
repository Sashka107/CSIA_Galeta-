<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" prefHeight="221.0" prefWidth="503.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.csia_galeta.controllers.AddJudgeController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane prefHeight="181.0" prefWidth="331.0">
      <children>
         <Button fx:id="addJudge" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#addJudge" text="Add Judge" />
         <TextField fx:id="judgeName" layoutX="55.0" layoutY="53.0" onMouseClicked="#handleClear"/>
         <TextField fx:id="judgeSurname" layoutX="74.0" layoutY="89.0" onMouseClicked="#handleClear"/>
         <Label layoutX="14.0" layoutY="53.0" prefHeight="25.0" prefWidth="39.0" text="Name:" />
         <Label layoutX="14.0" layoutY="89.0" prefHeight="25.0" prefWidth="60.0" text="Surname:" />

      </children>
   </AnchorPane>

</HBox>
