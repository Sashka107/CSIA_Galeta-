<?xml version="1.0" encoding="UTF-8"?>
<!-- XML declaration specifying encoding and version -->

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<!-- Imports for JavaFX classes used in the FXML -->

<AnchorPane prefHeight="413.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.csia_galeta.P2PController">
   <!-- AnchorPane element with specific attributes for preferred height, width, and controller class -->

   <ListView fx:id="pairListView" layoutX="17.0" layoutY="53.0" onMouseClicked="#handleClickOnPair" prefHeight="280.0" prefWidth="566.0" style="-fx-font-size: 20.0"/>
   <!-- ListView element for displaying a list of pairs with specific layout positions, size, and style, with an ID for referencing in the controller, and an event handler for mouse click -->

   <Label layoutX="260.0" layoutY="21.0" prefHeight="26.0" prefWidth="80.0" text="Pair Runs" fx:id="windowTitle">
      <!-- Label element for displaying "Pair Runs" with specific layout positions and size, with an ID for referencing in the controller -->
      <font>
         <Font size="20.0" />
         <!-- Font properties for the label text -->
      </font>
   </Label>

   <Button fx:id="saveAndExitBtn" layoutX="96.0" layoutY="348.0" mnemonicParsing="false" onAction="#saveAndExit" style="-fx-background-color: black;" text="Save and Exit" textFill="WHITE">
      <!-- Button element for saving and exiting with specific layout positions, action event handler, text, and style -->
      <font>
         <Font size="20.0" />
         <!-- Font properties for the button text -->
      </font>
   </Button>

   <Button fx:id="nextNet" onAction="#toNextNet" layoutX="264.0" layoutY="348.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="240.0" style="-fx-background-color: black;" text="Proceed to the next net" textFill="WHITE">
      <!-- Button element for proceeding to the next net with specific layout positions, action event handler, text, and style -->
      <font>
         <Font size="20.0" />
         <!-- Font properties for the button text -->
      </font>
   </Button>

</AnchorPane>
